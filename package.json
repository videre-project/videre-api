{
  "name": "videre-api",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.29.0",
    "jest": "27.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsdom": "^17.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.2",
    "postgres": "^1.0.2"
  },
  "scripts": {
    "dev": "docker-compose -f bin/docker-compose.yml up -d && nodemon --exec npm run start",
    "start": "nodemon --exec babel-node src",
    "build": "rimraf api && webpack",
    "test": "jest src --testTimeout=30000",
    "test-watch": "jest --watchAll --verbose --testTimeout=30000",
    "lint": "eslint src && prettier src --check",
    "lint-fix": "eslint src --fix && prettier src --write src"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
